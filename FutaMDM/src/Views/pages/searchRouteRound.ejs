<!DOCTYPE html>
<html lang="en">

<head>
	<%- include('../layouts/head.ejs') %>
</head>

<body>
	<%- include('../layouts/header.ejs',{userid}) %>
		<!-- Header End====================================================================== -->
		<div id="mainBody">
			<div class="container">
				<div class="row">
					<!-- Sidebar ================================================== -->
					<%- include('../layouts/sideBar.ejs') %>
						<!-- Sidebar end=============================================== -->
						<div class="span9">
							<ul class="breadcrumb">
								<li>Trang chủ <span class="divider">/</span></li>
								<li>Chuyến xe <span class="divider">/</span></li>
								<li class="active"><a href="/">
									<%= searchText %>
								</a></li>
							</ul>
							<div>
								<h2 style="color: #00613d; margin: 0 -15rem 10px;;">Chuyến đi</h2>
								<div><p style="margin: 0 -15rem 10px; color: #ef5222; font-size: 17px;">Không điểm dừng</p></div>
								<%- include('../partials/search/listSreach.ejs',{_listOneWay, moneyConvert}) %>
								<p style="margin: 0 -15rem 10px; color: #ef5222; font-size: 17px;">Một điểm dừng</p>
								<%- include('../partials/search/listSearchRoundA.ejs',{_routeTmp1, moneyConvert}) %>
							</div>

							<div>
								<h2 style="color: #00613d; margin: 0 -15rem 10px;">Chuyến về</h2>
								<p style="margin: 0 -15rem 10px; color: #ef5222; font-size: 17px;">Không điểm dừng</p>
								<%- include('../partials/search/listSreachRound.ejs',{_listReturn, moneyConvert}) %>
								<p style="margin: 0 -15rem 10px; color: #ef5222; font-size: 17px;">Một điểm dừng</p>
								<%- include('../partials/search/listSearchRoundB.ejs',{_routeTmp2, moneyConvert}) %>
							</div>
						</div>
					</div>
					
				</div>
		</div>
		<!-- MainBody End ============================= -->
		<!-- Footer ================================================================== -->
		<%- include('../layouts/footer.ejs') %>
			<!-- Placed at the end of the document so the pages load faster ============================================= -->
			<script src="themes/js/jquery.js" type="text/javascript"></script>
			<script src="themes/js/bootstrap.min.js" type="text/javascript"></script>
			<script src="themes/js/google-code-prettify/prettify.js"></script>

			<script src="themes/js/bootshop.js"></script>
			<script src="themes/js/jquery.lightbox-0.5.js"></script>
</body>

</html>